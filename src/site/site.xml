<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="Java Documentation Template"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0
         http://maven.apache.org/xsd/decoration-1.6.0.xsd">
	<bannerLeft>
		<name>${project.name}</name>
		<!-- change this to your own url -->
		<href>
			https://superevensteven.github.io/java-documentation-template/index.html
		</href>
	</bannerLeft>

	<publishDate position="right" format="MMMM dd, yyyy" />
	<version />

	<!-- Note: This skin is not compatible with mvn site pdf generation -->
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.7</version>
	</skin>

	<body>
		<menu name="Overview">
			<item name="Project README" href="index.html" />
			<item name="Technical Documentation"
				href="technical-documentation.html" />
		</menu>
		<menu ref="reports" />
		<footer />
	</body>

</project>
